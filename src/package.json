    "date-fns": "^1.29.0",
    "disqus-react": "^1.0.5",
    "gatsby": "^2.0.4",
    "gatsby-link": "^2.0.1",
    "gatsby-mdx": "^0.1.4",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-sharp": "^2.1.1",
    "katex": "^0.10.0-rc.1",
    "lodash": "^4.17.11",
    "natural-sort": "^1.0.0",
    "polished": "^2.0.3",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emoji-render": "^0.4.5",
    "react-helmet": "^5.2.0",
    "react-player": "^1.6.4",
    "react-typography": "^0.16.13",
    "styled-components": "beta",
    "typography": "^0.16.17",
    "typography-theme-lawton": "^0.15.10"
  },
  "keywords": [
    "gatsby",
    "math",
    "machine learning",
    "blog"
  ],
  "scripts": {
    "build": "yarn prebuild && gatsby build",
    "clean": "find .. -maxdepth 1 ! -name src ! -name README.md ! -name . ! -name .. ! -name .git ! -name .gitignore -exec rm -rf {} \\; && rm -rf public && rm -rf .cache",
    "develop": "yarn prebuild && gatsby develop",
    "deploy": "yarn build && cp -rf public/* .. && git add -A && git commit -am 'Deploy' && git push",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "prebuild": "mkdir -p public/src && cp -rf src/favicons public/src/",
    "prune": "yarn clean && rm -rf node_modules",
    "serve": "gatsby serve",
    "ssr": "yarn build && yarn serve"
  },
  "devDependencies": {
    "prettier": "^1.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
