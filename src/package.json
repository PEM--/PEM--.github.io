  "keywords": [
    "gatsby",
    "math",
    "machine learning",
    "blog"
  ],
  "scripts": {
    "build": "gatsby build",
    "clean:cache": "rm -rf public && rm -rf .cache",
    "clean:deploy": "find .. -maxdepth 1 ! -name src ! -name README.md ! -name . ! -name .. ! -name .git ! -name .gitignore -exec rm -rf {} \\;",
    "develop": "gatsby develop",
    "deploy": "yarn clean:deploy && yarn build && cp -rf public/* .. && git add -A && git commit -am 'Deploy' && git push",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint *.js src/**/*.js",
    "prune": "yarn clean:cache && rm -rf node_modules",
    "serve": "gatsby serve",
    "ssr": "yarn build && yarn serve"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
