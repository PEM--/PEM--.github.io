{
  "name": "pem--.github.io",
  "license": "MIT",
  "author": "Pierre-Eric Marchandet <pemarchandet@gmail.com>",
  "description": "PEM's blog",
  "version": "1.0.0",
  "dependencies": {
    "@reach/router": "^1.1.1",
    "babel-plugin-styled-components": "^1.7.1",
    "gatsby": "next",
    "gatsby-link": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-styled-components": "next",
    "gatsby-plugin-typography": "next",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "styled-components": "latest",
    "typography": "^0.16.17",
    "typography-theme-lawton": "^0.15.10"
  },
  "keywords": [
    "gatsby",
    "math",
    "machine learning",
    "blog"
  ],
  "scripts": {
    "clean": "find .. -maxdepth 1 ! -name src ! -name README.md ! -name . ! -name .. ! -name .git ! -name .gitignore -exec rm -rf {} \\; && rm -rf public",
    "develop": "gatsby develop",
    "deploy": "yarn build && cp -rf public/* .. && git add -A && git commit -am 'Deploy' && git push",
    "serve": "gatsby serve",
    "ssr": "yarn build && yarn serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build": "gatsby build"
  },
  "devDependencies": {
    "prettier": "^1.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
